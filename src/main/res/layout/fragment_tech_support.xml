<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.rohithreddy.hkwikmint.TechSupport">

    <!-- TODO: Update blank fragment layout -->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/l1"
        android:orientation="vertical"
        android:layout_alignParentBottom="true"
        android:id="@+id/linearLayout">
        <android.support.v7.widget.CardView
            android:id="@+id/card_view"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_margin="8dp">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

            <TextView
                android:id="@+id/name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Sriram Reddy"
                android:textStyle="bold"
                android:padding="5dp"
                android:textSize="25sp"/>
            <TextView
                android:id="@+id/position"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/name"
                android:text="Manager"
                android:textStyle="bold"
                android:textSize="20sp"/>
                <TextView
                    android:id="@+id/phonenumber"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/position"
                    android:text="9000091777"
                    android:textStyle="bold"
                    android:textSize="20sp"/>
                <android.support.design.widget.CheckableImageButton
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:background="@drawable/ic_phone_green_600_24dp"
                    android:layout_marginEnd="22dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true" />

            </RelativeLayout>
        </android.support.v7.widget.CardView>


        <android.support.v7.widget.CardView
            android:id="@+id/card_view1"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_margin="8dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/name1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Bhogesh"
                    android:textStyle="bold"
                    android:padding="5dp"
                    android:textSize="25sp"/>
                <TextView
                    android:id="@+id/position1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/name1"
                    android:text="Developer"
                    android:textStyle="bold"
                    android:textSize="20sp"/>
                <TextView
                    android:id="@+id/phonenumber1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/position1"
                    android:text="8967754309"
                    android:textStyle="bold"
                    android:textSize="20sp"/>
                <android.support.design.widget.CheckableImageButton
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:background="@drawable/ic_phone_green_600_24dp"
                    android:layout_marginEnd="22dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true" />

            </RelativeLayout>
        </android.support.v7.widget.CardView>

        <android.support.v7.widget.CardView
            android:id="@+id/card_view2"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_margin="8dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/name2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="k Rohith reddy"
                    android:textStyle="bold"
                    android:padding="5dp"
                    android:textSize="25sp"/>
                <TextView
                    android:id="@+id/position2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/name2"
                    android:text="Developer"
                    android:textStyle="bold"
                    android:textSize="20sp"/>
                <TextView
                    android:id="@+id/phonenumber2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/position2"
                    android:text="9502177727"
                    android:textStyle="bold"
                    android:textSize="20sp"/>
                <android.support.design.widget.CheckableImageButton
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:background="@drawable/ic_phone_green_600_24dp"
                    android:layout_marginEnd="22dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true" />

            </RelativeLayout>
        </android.support.v7.widget.CardView>


    </LinearLayout>

</FrameLayout>
