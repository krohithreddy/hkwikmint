<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.rohithreddy.hkwikmint.Sync">

    <!-- TODO: Update blank fragment layout -->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/l1"
        android:orientation="vertical"
        android:layout_alignParentBottom="true"
        android:id="@+id/linearLayout">

        <android.support.v7.widget.CardView
            android:id="@+id/card_view0"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="8dp">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <android.support.design.widget.CheckableImageButton
                    android:id="@+id/check0"
                    android:layout_width="40dp"
                    android:layout_height="50dp" />

                <TextView
                    android:id="@+id/position0"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Sync All"
                    android:textStyle="bold"
                    android:textAlignment="center"
                    android:textSize="28sp"
                    android:layout_alignParentTop="true" />
                <TextView
                    android:id="@+id/phonenumber0"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/position0"
                    android:textAlignment="center"
                    android:text="lastSynced at"
                    android:textStyle="bold"
                   />

                <android.support.design.widget.CheckableImageButton
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:background="@drawable/ic_sync_black_24dp"
                    android:layout_marginEnd="22dp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"
                    android:id="@+id/checkableImageButton0" />

            </RelativeLayout>
        </android.support.v7.widget.CardView>


        <android.support.v7.widget.CardView
            android:id="@+id/card_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="8dp">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <android.support.design.widget.CheckableImageButton
                    android:id="@+id/check"
                    android:layout_width="40dp"
                    android:layout_height="50dp" />

                <TextView
                    android:id="@+id/position"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Manager"
                    android:textStyle="bold"
                    android:textSize="28sp"
                    android:layout_alignParentTop="true"
                    android:layout_toEndOf="@+id/check"
                    android:layout_marginStart="16dp" />
                <TextView
                    android:id="@+id/phonenumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/position"
                    android:layout_toEndOf="@+id/check"

                    android:text="lastSynced at"
                    android:textStyle="bold"
                    android:textSize="15sp"
                    android:layout_marginStart="16dp" />

                <android.support.design.widget.CheckableImageButton
                    android:layout_width="40dp"
